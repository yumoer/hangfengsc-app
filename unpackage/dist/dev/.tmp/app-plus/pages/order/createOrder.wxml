<view><navigator class="address-section" url="/pages/address/address?source=1"><view class="order-content"><text class="yticon icon-shouhuodizhi"></text><view class="cen"><view class="top"><text class="name">{{addressData.title}}</text><text class="mobile">{{addressData.mobile}}</text></view><block wx:if="{{addressData.addressName!==undefined}}"><text class="address">{{addressData.addressName+" "+addressData.place}}</text></block></view><text class="yticon icon-you"></text></view></navigator><view class="goods-section"><view class="g-header b-b"><text class="name">商品列表</text></view><block wx:for="{{datas}}" wx:for-item="data" wx:for-index="index" wx:key="index"><block wx:if="{{datas.length>0}}"><view class="g-item"><image src="{{data.default_image_url}}"></image><view class="right"><text class="title clamp">{{data.name}}</text><view class="price-box" style="margin-top:20px;"><text class="price" style="color:red;">{{"￥"+data.price}}</text><text class="number" style="float:right;display:inherit;">{{"x "+data.count}}</text></view></view></view></block></block><block wx:if="{{datas.length===undefined}}"><view class="g-item"><image src="{{datas.default_image_url}}"></image><view class="right"><text class="title clamp">{{datas.name}}</text><view class="price-box" style="margin-top:20px;"><text class="price" style="color:red;">{{"￥"+datas.price}}</text><text class="number" style="float:right;display:inherit;">{{"x "+datas.count}}</text></view></view></view></block></view><view class="yt-list"><navigator class="yt-list-cell b-b" url="/pages/invoice/invoice?source=1"><view class="cell-icon">票</view><text class="cell-tit clamp">发票信息</text><text class="cell-tip active" style="margin-right:5px;">{{invoice}}</text><text class="yticon icon-you" style="font-size:14px;line-height:20px;"></text><text class="cell-more wanjia wanjia-gengduo-d"></text></navigator><block wx:if="{{couponList.id!==undefined}}"><view class="uni-list"><view class="pay-type-list"><view class="type-item b-b"><view class="con"><text class="tit">{{couponList.invoice_title}}</text><text>{{couponList.invoice_org_code}}</text></view><view class="right"><block wx:if="{{couponList.invoice_bank===''||couponList.invoice_bank_code===''||couponList.invoice_phone===''||couponList.register_address===''}}"><text class="price" style="color:#fa436a;font-size:14px;">普票|电子票</text></block><block wx:else><text class="price" style="color:#fa436a;font-size:14px;">增值税发票</text></block><text data-event-opts="{{[['tap',[['delInvoice']]]]}}" class="del-btn yticon icon-fork" style="margin-left:20rpx;" bindtap="__e"></text></view></view></view></view></block><view class="yt-list-cell desc-cell"><text class="cell-tit clamp">发票邮寄地址</text><input class="desc" type="text" placeholder="请填写发票具体邮寄地址" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','send_invoice_address','$event',[]],['info']]]]]}}" value="{{info.send_invoice_address}}" bindinput="__e"/></view></view><view class="yt-list"><view class="yt-list-cell b-b"><text class="cell-tit clamp">商品金额</text><block wx:if="{{!price}}"><text class="cell-tip">{{"￥"+$root.m0*count}}</text></block><block wx:else><text class="cell-tip">{{"￥"+$root.m1}}</text></block></view><view class="yt-list-cell b-b"><text class="cell-tit clamp">运费</text><block wx:if="{{price>=99}}"><text class="cell-tip">免运费</text></block><block wx:else><text class="cell-tip">￥13.00</text></block></view><view class="yt-list-cell desc-cell"><text class="cell-tit clamp">备注</text><input class="desc" type="text" placeholder="请填写备注信息" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['','desc','$event',[]]]]]]}}" value="{{desc}}" bindinput="__e"/></view></view><view class="footer"><view class="price-content"><text>实付款</text><text class="price-tip">￥</text><block wx:if="{{price>=99}}"><text><block wx:if="{{!price}}"><text class="price">{{$root.m2*count}}</text></block><block wx:else><text class="price">{{$root.m3}}</text></block></text></block><block wx:else><text><block wx:if="{{!price}}"><text class="price">{{$root.m4*count+13.00}}</text></block><block wx:else><text class="price">{{$root.m5+13.00}}</text></block></text></block></view><text data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="submit" bindtap="__e">提交订单</text></view></view>