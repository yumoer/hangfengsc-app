<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','receiver','$event',[]],['addressData']]]]]}}" value="{{addressData.receiver}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['addressData']]]]]}}" value="{{addressData.mobile}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地址</text><view data-event-opts="{{[['tap',[['chooseCity',['$event']]]]]}}" class="input" bindtap="__e">{{''+addressData.addressName+''}}</view><text class="yticon icon-shouhuodizhi"></text></view><view class="row b-b"><text class="tit">门牌号</text><input class="input" type="text" placeholder="楼号、门牌" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','place','$event',[]],['addressData']]]]]}}" value="{{addressData.place}}" bindinput="__e"/></view><block wx:if="{{default_address_id}}"><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.default}}" color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view></block><button data-event-opts="{{[['tap',[['confirm',['$0'],['addressData']]]]]}}" class="add-btn" bindtap="__e">提交</button><button data-event-opts="{{[['tap',[['condelete',['$0'],['addressData']]]]]}}" class="del-btn" bindtap="__e">删除</button><mpvue-city-picker class="vue-ref" vue-id="0e0a4db1-1" themeColor="{{themeColor}}" pickerValueDefault="{{cityPickerValue}}" data-ref="mpvueCityPicker" data-event-opts="{{[['^onCancel',[['onCancel']]],['^onConfirm',[['onConfirm']]]]}}" bind:onCancel="__e" bind:onConfirm="__e" bind:__l="__l"></mpvue-city-picker></view>