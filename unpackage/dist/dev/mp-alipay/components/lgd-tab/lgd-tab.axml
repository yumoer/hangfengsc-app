<view><scroll-view class="scrollView" scroll-x="{{true}}" show-scrollbar="false" scroll-left="{{scrollLeft}}" scroll-with-animation="{{true}}"><view class="tabBox" style="{{'justify-content:'+(isOutWindow?'':'space-around')+';'}}"><view data-event-opts="{{[['tap',[['clickTab',[index]]]]]}}" class="items" onTap="__e" a:for="{{tabValue}}" a:for-item="item" a:for-index="index" a:key="index"><text class="{{(('tabText')+' '+(index==tIndex?'active':''))}}" style="{{'font-size:'+(fontSize+'px')+';'+('color:'+(index==tIndex?textColor:'')+';')}}">{{''+item+''}}</text></view></view><view class="underscore" style="{{'width:'+(inderWidth+'px')+';'+('margin-left:'+(indexLeft+boxLeft+'px')+';')+('background-color:'+(textColor)+';')+('height:'+('3px')+';')}}"></view></scroll-view></view>