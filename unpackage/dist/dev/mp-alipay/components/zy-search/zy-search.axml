<view style="padding:0 20px;" class="data-v-c880309c"><view class="search data-v-c880309c"><block a:if="{{isFocus}}"><input maxlength="20" focus="{{true}}" type="text" value="{{searchText}}" confirm-type="search" placeholder="请输入关键词搜索" data-event-opts="{{[['confirm',[['searchStart']]],['input',[['__set_model',['','searchText','$event',['trim']]]]],['blur',[['$forceUpdate']]]]}}" onConfirm="__e" onInput="__e" onBlur="__e" class="data-v-c880309c"/></block><block a:else><input maxlength="20" type="text" value="{{searchText}}" confirm-type="search" placeholder="请输入关键词搜索" data-event-opts="{{[['confirm',[['searchStart']]],['input',[['__set_model',['','searchText','$event',['trim']]]]],['blur',[['$forceUpdate']]]]}}" onConfirm="__e" onInput="__e" onBlur="__e" class="data-v-c880309c"/></block><image class="search-icon data-v-c880309c" src="../../static/zy-search/search.svg" mode="aspectFit" data-event-opts="{{[['tap',[['searchStart']]]]}}" onTap="__e"></image></view><block a:if="{{hList.length>0}}"><view class="{{(('data-v-c880309c')+' '+('s-'+theme))}}"><view class="header data-v-c880309c">历史搜索<image src="../../static/zy-search/delete.svg" mode="aspectFit" data-event-opts="{{[['tap',[['delhistory',['$event']]]]]}}" onTap="__e" class="data-v-c880309c"></image></view><view class="list data-v-c880309c"><view data-event-opts="{{[['tap',[['keywordsClick',['$0'],[[['hList','',index]]]]]]]}}" onTap="__e" class="data-v-c880309c" a:for="{{hList}}" a:for-item="item" a:for-index="index" a:key="index">{{item}}</view></view></view></block><block a:if="{{showWant}}"><view class="{{(('data-v-c880309c')+' '+('wanted-'+theme))}}"><view class="header data-v-c880309c">搜索发现</view><view class="list data-v-c880309c"><view data-event-opts="{{[['tap',[['keywordsClick',['$0'],[[['hotList','',index]]]]]]]}}" onTap="__e" class="data-v-c880309c" a:for="{{hotList}}" a:for-item="item" a:for-index="index" a:key="index">{{item}}</view></view></view></block></view>