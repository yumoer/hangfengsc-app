#page {
  width: 100%;
  height: 100%;
  background-color: transparent;
  padding: 20px;
}
#page .banner {
  width: 100%;
  height: 180px;
  margin: 0 auto;
}
#page .banner ._img {
  border-radius: 10px;
}
#page .content {
  width: 100%;
  height: 100%;
}
#page .content .mobile {
  width: 100%;
  height: 60px;
  margin-top: 20px;
}
#page .content .mobile .left {
  width: 35%;
  height: 100%;
  float: left;
  background-color: #ddd;
  border-radius: 10px;
  position: relative;
}
#page .content .mobile .left text {
  display: block;
  font-size: 16px;
  text-align: center;
  line-height: 60px;
}
#page .content .mobile .right {
  width: 63%;
  height: 100%;
  float: right;
  background-color: #ddd;
  border-radius: 10px;
}
#page .content .mobile .right text {
  display: block;
  font-size: 16px;
  text-align: center;
  line-height: 60px;
}
#page .content .qq {
  width: 100%;
  height: 60px;
  margin-top: 20px;
}
#page .content .qq .left {
  width: 35%;
  height: 100%;
  float: left;
  background-color: #f5f5f5;
  border-radius: 10px;
  position: relative;
}
#page .content .qq .left text {
  display: block;
  font-size: 16px;
  text-align: center;
  line-height: 60px;
}
#page .content .qq .right {
  width: 63%;
  height: 100%;
  float: right;
  background-color: #f5f5f5;
  border-radius: 10px;
}
#page .content .qq .right text {
  display: block;
  font-size: 16px;
  text-align: center;
  line-height: 60px;
}

