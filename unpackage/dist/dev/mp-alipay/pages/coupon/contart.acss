page {
  padding: 0px;
}
.page_box {
  padding: 30rpx;
}
.icon-jia2 {
  font-size: 60rpx;
  display: flex;
  justify-content: center;
  line-height: 140rpx;
  color: #999;
}
.form-item {
  background-color: #fff;
  border-radius: 20rpx;
  padding: 40rpx;
  margin-bottom: 20rpx;
}
.form-item .inp-title {
  font-size: 32rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: #333;
}
.form-item .mb {
  margin-bottom: 40rpx;
}
.form-item .mb1 {
  margin-bottom: 20rpx;
}
.form-item .inp {
  height: 60rpx;
  background: #fff;
  border-bottom: 1px solid #ddd;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333;
  margin-top: 20rpx;
}
.form-item .inp-house {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #b1b3c7;
}
.form-item .area-inp {
  height: 190rpx;
  padding: 30rpx;
}
.form-item .pl-style {
  font-size: 28rpx;
  font-family: PingFang SC;
  color: #b1b3c7;
}
.area-box {
  min-height: 206rpx;
  border-radius: 20rpx;
}
.area-box .pl-style {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #b1b3c7;
  line-height: 50rpx;
}
.area-box .inp-area {
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #333;
  line-height: 50rpx;
  width: 100%;
  padding: 10rpx 30rpx;
  border-radius: 20rpx;
  border: 1px solid #ddd;
}
.area-box .img-box {
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.area-box .img-box .choose-img,
.area-box .img-box .preview-box {
  width: 140rpx;
  height: 140rpx;
  background: #fff;
  border: 1rpx solid #dfdfdf;
  position: relative;
}
.area-box .img-box .choose-img:nth-child(5n),
.area-box .img-box .preview-box:nth-child(5n) {
  margin-right: 0;
}
.area-box .img-box .choose-img .cuIcon-cameraadd,
.area-box .img-box .preview-box .cuIcon-cameraadd {
  font-size: 50rpx;
  color: #dfdfdf;
}
.area-box .img-box .choose-img .preview-img,
.area-box .img-box .preview-box .preview-img {
  width: 100%;
  height: 100%;
}
.area-box .img-box .choose-img .cuIcon-close,
.area-box .img-box .preview-box .cuIcon-close {
  background: #fa436a;
  border-radius: 50%;
  width: 40rpx;
  line-height: 40rpx;
  color: #fff;
  text-align: center;
  position: absolute;
  top: -10rpx;
  right: -10rpx;
}
.foot_box {
  display: flex;
  height: 80rpx;
  width: 100%;
  position: fixed;
  bottom: 60rpx;
}
.foot_box .contact-btn {
  flex: 1;
  height: 70rpx;
  background-image: linear-gradient(to right, #EE1D23 0%, #F04023 100%);
  box-shadow: 0px 2rpx 5rpx 0px rgba(102, 103, 104, 0.46);
  border-radius: 35rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #ffffff;
}
.foot_box .post-btn {
  height: 80rpx;
  width: 590rpx;
  background-image: linear-gradient(to right, #EE1D23 0%, #F04023 100%);
  box-shadow: 0px 7rpx 6rpx 0rpx rgba(229, 138, 0, 0.22);
  border-radius: 37rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #ffffff;
}
.radio-box {
  margin-top: 30rpx;
}
.radio-box .radio-item {
  margin-bottom: 40rpx;
}
.radio-box .radio-item .radio-inp {
  -webkit-transform: scale(0.7);
          transform: scale(0.7);
  margin-right: 10rpx;
}
.radio-box .radio-item .radio-title {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #333333;
}
.uni-radio-input-checked {
  background-color: #f37b1d !important;
  border: #f37b1d !important;
}

