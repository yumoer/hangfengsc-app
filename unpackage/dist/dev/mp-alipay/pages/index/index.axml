<view class="container"><view class="carousel-section"><view class="titleNview-placing"></view><view class="titleNview-background"></view><swiper class="carousel" circular="{{true}}" autoplay="{{true}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" onChange="__e"><swiper-item data-event-opts="{{[['tap',[['goToPage',['$0'],[[['carouselList','',index,'url']]]]]]]}}" class="carousel-item" onTap="__e" a:for="{{carouselList}}" a:for-item="item" a:for-index="index" a:key="index"><image src="{{item.image}}"></image></swiper-item></swiper><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view></view><view class="cate-section"><u-row style="width:100%;" vue-id="8deb4120-1" gutter="16" onVueInit="__l"><u-col vue-id="{{('8deb4120-2-'+index)+','+('8deb4120-1')}}" span="3" a:for="{{homePageList}}" a:for-item="homePage" a:for-index="index" a:key="index" onVueInit="__l"><view data-event-opts="{{[['tap',[['goIcon',['$0'],[[['homePageList','',index]]]]]]]}}" class="cate-item" onTap="__e"><image src="{{homePage.src}}"></image><text>{{homePage.text}}</text></view></u-col></u-row></view><view class="func-wrapper"><view class="func-content"><swiper style="height:285px;" indicator-dots="{{true}}" autoplay="{{false}}" interval="{{3000}}" duration="{{1000}}"><block a:for="{{goodsShopList}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{index<5}}"><swiper-item><view data-event-opts="{{[['tap',[['navToPage',['/pages/activity/hotProd']]]]]}}" class="swiper-item" onTap="__e"><text class="swiper-text">今日爆款</text><image class="swiper-image" src="{{item.image}}" mode></image></view></swiper-item></block></block></swiper></view><view class="func-right"><view data-event-opts="{{[['tap',[['navToPage',['/pages/activity/newProd']]]]]}}" class="func-xpsf" onTap="__e"><image class="func-image" src="http://47.94.106.106:8888/group1/M00/52/E4/rBHxiGEDy4yAPoSpAABQXZsaSGs8552342"