page {
  font-size: 16px;
}
page {
  margin: 0;
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Oxygen', 'Ubuntu', 'Cantarell', 'Fira Sans', 'Droid Sans', 'Helvetica Neue', 'Microsoft Yahei', sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
view,
image,
text {
  box-sizing: border-box;
  flex-shrink: 0;
}
#app {
  width: 100vw;
  height: 100vh;
}
.flex-row {
  display: flex;
  flex-direction: row;
}
.flex-col {
  display: flex;
  flex-direction: column;
}
.justify-start {
  display: flex;
  justify-content: flex-start;
}
.justify-center {
  display: flex;
  justify-content: center;
}
.justify-end {
  display: flex;
  justify-content: flex-end;
}
.justify-evenly {
  display: flex;
  justify-content: space-evenly;
}
.justify-around {
  display: flex;
  justify-content: space-around;
}
.justify-between {
  display: flex;
  justify-content: space-between;
}
.items-start {
  display: flex;
  align-items: flex-start;
}
.items-center {
  display: flex;
  align-items: center;
}
.items-end {
  display: flex;
  align-items: flex-end;
}
.wrap {
  width: 100%;
  height: 100%;
}
.wrap .wrap-header {
  width: 100%;
  height: 570rpx;
  background: url("http://47.94.106.106:8888/group1/M00/53/87/rBHxiGEKRgKAUwsqAAu6_A43jKo0155421") no-repeat top center/100%;
}
.wrap .wrap-header .header {
  padding-top: 380rpx;
  padding-left: 30rpx;
}
.wrap .wrap-header .header .header-name {
  color: #FFFFFF;
  font-size: 36rpx;
  margin-bottom: 10rpx;
  font-weight: bold;
}
.wrap .wrap-header .header .header-info {
  color: #FFFFFF;
  font-size: 24rpx;
}
.wrap .company {
  width: 100%;
  background-color: #fff;
  position: relative;
}
.wrap .company .company-like {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  background-color: #fff;
  box-shadow: 0 0 7rpx 0 rgba(238, 29, 35, 0.1);
  position: absolute;
  right: 15rpx;
  top: -60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  --animate-duration: 1s;
  --animate-delay: 1s;
  --animate-repeat: 1;
}
.wrap .company .company-like .icon {
  width: 58rpx;
  height: 50rpx;
}
.wrap .company .company-like .animate_heartBeat {
  -webkit-animation-name: heartBeat;
          animation-name: heartBeat;
  -webkit-animation-duration: calc(1s * 1.3);
          animation-duration: calc(1s * 1.3);
  -webkit-animation-duration: calc(var(--animate-duration) * 1.3);
          animation-duration: calc(var(--animate-duration) * 1.3);
  -webkit-animation-timing-function: ease-in-out;
          animation-timing-function: ease-in-out;
  -webkit-animation-iteration-count: infinite;
          animation-iteration-count: infinite;
}
@-webkit-keyframes heartBeat {
0% {
    -webkit-transform: scale(1);
            transform: scale(1);
}
14% {
    -webkit-transform: scale(1.3);
            transform: scale(1.3);
}
28% {
    -webkit-transform: scale(1);
            transform: scale(1);
}
42% {
    -webkit-transform: scale(1.3);
            transform: scale(1.3);
}
70% {
    -webkit-transform: scale(1);
            transform: scale(1);
}
}
@keyframes heartBeat {
0% {
    -webkit-transform: scale(1);
            transform: scale(1);
}
14% {
    -webkit-transform: scale(1.3);
            transform: scale(1.3);
}
28% {
    -webkit-transform: scale(1);
            transform: scale(1);
}
42% {
    -webkit-transform: scale(1.3);
            transform: scale(1.3);
}
70% {
    -webkit-transform: scale(1);
            transform: scale(1);
}
}
.wrap .websiteWrapper {
  width: 100%;
  padding: 81rpx 30rpx 30rpx;
  box-sizing: border-box;
}
.wrap .websiteWrapper .title {
  color: #333;
  font-size: 32rpx;
  font-weight: bold;
}
.wrap .websiteWrapper .eachWebsite {
  width: 100%;
  height: 140rpx;
  box-sizing: border-box;
  padding: 25rpx 0 25rpx 30rpx;
  background-color: #fff;
  box-shadow: 0 0 10rpx rgba(35, 4, 5, 0.1);
  border-radius: 10rpx;
  margin-top: 30rpx;
  display: flex;
  justify-content: flex-start;
  position: relative;
}
.wrap .websiteWrapper .eachWebsite .name {
  width: calc(100% - 300rpx);
  height: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  color: #666;
  font-size: 20rpx;
  padding-left: 36rpx;
  box-sizing: border-box;
}
.wrap .websiteWrapper .eachWebsite .count {
  position: absolute;
  width: 105rpx;
  background: url("http://47.94.106.106:8888/group1/M00/5C/C9/rBHxiGFmfvqAfUkzAAAHjhblS_M8760217") no-repeat top center/100%;
  height: 100%;
  right: -8rpx;
  top: -8rpx;
  padding-top: 18rpx;
  padding-left: 60rpx;
  box-sizing: border-box;
  color: #fff;
  font-weight: bold;
  font-size: 32rpx;
}

