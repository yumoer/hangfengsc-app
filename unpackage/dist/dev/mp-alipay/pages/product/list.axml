<view class="content"><view class="navbar" style="{{'position:'+(headerPosition)+';'}}"><view data-event-opts="{{[['tap',[['tabClick',[0]]]]]}}" class="{{(('nav-item')+' '+((filterIndex===0)?'current':''))}}" onTap="__e">默认排序</view><view data-event-opts="{{[['tap',[['tabClick',[1]]]]]}}" class="{{(('nav-item')+' '+((filterIndex===1)?'current':''))}}" onTap="__e">价格高低</view><view data-event-opts="{{[['tap',[['tabClick',[2]]]]]}}" class="{{(('nav-item')+' '+((filterIndex===2)?'current':''))}}" onTap="__e">人气高低</view></view><view class="goods"><view class="guess-section"><view data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['goodsList','',index]]]]]]]}}" class="guess-item" onTap="__e" a:for="{{goodsList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="image-wrapper"><image src="{{item.detail_image}}" mode="aspectFill"></image></view><view class="info"><text class="title">{{item.name}}</text><text><text class="price">{{item.price}}</text><text class="sales">{{item.sales+"人付款"}}</text></text></view></view></view></view><uni-load-more vue-id="5062fff6-1" status="{{loadingType}}" onVueInit="__l"></uni-load-more><view data-event-opts="{{[['tap',[['toggleCateMask',['$event']]]]]}}" class="{{(('cate-mask')+' '+(cateMaskState===0?'none':cateMaskState===1?'show':''))}}" onTap="__e"><view data-event-opts="{{[['tap',[['stopPrevent',['$event']]]],['touchMove',[['stopPrevent',['$event']]]]]}}" class="cate-content" catchTap="__e" catchTouchMove="__e"><scroll-view class="cate-list" scroll-y="{{true}}"><view a:for="{{cateList}}" a:for-item="item" a:for-index="__i0__" a:key="id"><view class="cate-item b-b two">{{item.name}}</view><view data-event-opts="{{[['tap',[['changeCate',['$0'],[[['cateList','id',item.id],['child','id',tItem.id]]]]]]]}}" class="{{((('cate-item')+' '+'b-b')+' '+((tItem.id==cateId)?'active':''))}}" onTap="__e" a:for="{{item.child}}" a:for-item="tItem" a:for-index="__i1__" a:key="id">{{''+tItem.name+''}}</view></view></scroll-view></view></view></view>