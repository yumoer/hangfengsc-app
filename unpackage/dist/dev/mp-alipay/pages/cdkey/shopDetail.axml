<view><swiper class="carousel" circular="{{true}}" autoplay="{{true}}" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" onChange="__e"><swiper-item data-event-opts="{{[['tap',[['into',['$0'],[[['carouselList','',index]]]]]]]}}" class="carousel-item" onTap="__e" a:for="{{carouselList}}" a:for-item="item" a:for-index="index" a:key="index"><image style="width:100%;height:400px;" src="{{item.image}}"></image></swiper-item></swiper><view class="swiper-dots"><text class="num">{{swiperCurrent+1}}</text><text class="sign">/</text><text class="num">{{swiperLength}}</text></view><view style="margin:20px 10px;"><text style="font-size:16px;">{{exchanges.name}}</text><view class="_br"></view></view><view style="margin:20px 10px;"><text style="font-size:14px;color:#999;">{{exchanges.explain}}</text></view><block a:if="{{exchanges.detail_image!==undefined}}"><image style="width:100%;height:8000px;padding:10px;" src="{{exchanges.detail_image}}" mode></image></block></view>