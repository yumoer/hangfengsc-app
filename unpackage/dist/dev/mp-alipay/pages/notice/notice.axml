<view><view data-event-opts="{{[['tap',[['navTo',['$0'],[[['notices','',index,'id']]]]]]]}}" class="notice-item" onTap="__e" a:for="{{notices}}" a:for-item="notice" a:for-index="index" a:key="index"><text class="time">{{notice.create_time}}</text><view class="content"><text class="title">{{notice.name}}</text><block a:if="{{notice.image}}"><view class="img-wrapper"><image class="pic" src="{{notice.image}}"></image></view></block><block a:if="{{notice.decs}}"><text class="introduce">{{notice.decs}}</text></block><view class="bot b-t"><text>查看详情</text><text class="more-icon yticon icon-you"></text></view></view></view></view>