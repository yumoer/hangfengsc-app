.d-card {
  width: 100%;
  height: 460rpx;
  padding: 50rpx 30rpx;
  background-color: #f8f8f8;
}
.d-card .d-wrap {
  width: 100%;
  height: 360rpx;
  background: url("http://47.94.106.106:8888/group1/M00/5D/27/rBHxiGGtrpOAb-QjAAKLT9IVng03259065");
  background-size: 100% 360rpx;
  color: #fff;
  position: relative;
}
.d-card .d-wrap .d-title {
  position: absolute;
  top: 96rpx;
  right: 40rpx;
  font-size: 12px;
}
.d-card .d-wrap .d-money {
  position: absolute;
  top: 140rpx;
  right: 40rpx;
  font-size: 30px;
}
.d-card .d-wrap .d-ka {
  position: absolute;
  bottom: 40rpx;
  left: 40rpx;
}
.d-card .d-wrap .d-num {
  position: absolute;
  bottom: 40rpx;
  right: 40rpx;
}
.d-content {
  width: 100%;
  height: 300px;
  background-color: #fff;
}
.d-content .d-name {
  margin-top: 100rpx;
  text-align: center;
}
.d-content .d-input {
  text-align: center;
  margin-top: 53px;
}
.d-content .d-input .input {
  height: 60rpx;
  width: 80%;
  margin: 0 10%;
  border-bottom: 2rpx solid #ddd;
  font-size: 14px;
}
.d-content .d-red {
  text-align: center;
  margin-top: 20rpx;
}
.d-content .d-red .d-text {
  color: #F1654E;
  font-size: 12px;
  margin-top: 20px;
}
.d-content .m-wrap {
  position: absolute;
  bottom: 100rpx;
  width: 100%;
}
.d-content .m-wrap .d-btn {
  width: 590rpx;
  margin: 0 auto;
  font-weight: bold;
  background: linear-gradient(to right, #EE1D23 0%, #F04023 100%);
  color: #fff;
  border-radius: 40rpx;
}

