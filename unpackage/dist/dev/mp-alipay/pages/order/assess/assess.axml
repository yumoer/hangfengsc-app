<view class="content"><view class="order-item"><view class="goods-box-single"><image class="goods-img" src="{{goodsItem.image}}" mode="aspectFill"></image><view class="right"><text class="title clamp2">{{goodsItem.title}}</text><view class="other"><text class="price" style="float:left;">{{goodsItem.price+''}}</text><text class="attr-box" style="float:right;">{{'x '+goodsItem.count}}</text></view></view></view><view class="goods-box-assess"><u-input onInput="__e" vue-id="2fea5812-1" type="{{type}}" border="{{border}}" height="{{height}}" auto-height="{{autoHeight}}" placeholder="请为这个商品添加您的评价" value="{{infoReceive.textareaValue}}" data-event-opts="{{[['^input',[['__set_model',['$0','textareaValue','$event',[]],['infoReceive']]]]]}}" onVueInit="__l"></u-input><view class="rate"><text class="rate_title">商品评分</text><u-rate onInput="__e" vue-id="2fea5812-2" count="{{count}}" value="{{infoReceive.score}}" data-event-opts="{{[['^input',[['__set_model',['$0','score','$event',[]],['infoReceive']]]]]}}" onVueInit="__l"></u-rate></view><view class="assess"><u-checkbox-group vue-id="2fea5812-3" onVueInit="__l"><u-checkbox vue-id="{{('2fea5812-4-'+index)+','+('2fea5812-3')}}" name="{{item.name}}" active-color="#ee1d23" value="{{infoReceive.is_anonymous}}" data-event-opts="{{[['^change',[['checkboxChange']]],['^input',[['__set_model',['$0','is_anonymous','$event',[]],['infoReceive']]]]]}}" onChange="__e" onInput="__e" a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index" onVueInit="__l">{{item.name}}</u-checkbox></u-checkbox-group></view></view></view><view class="confirm"><u-button vue-id="2fea5812-5" type="error" data-event-opts="{{[['^click',[['confirmBtn',['$0'],['infoReceive']]]]]}}" onClick="__e" onVueInit="__l">发表评价</u-button></view></view>