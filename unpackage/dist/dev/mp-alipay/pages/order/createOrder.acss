  position: absolute;
  left: 0;
  bottom: 0;
  content: '';
  width: 100%;
  height: 0;
  border-bottom: 1px dashed #f3f3f3;
  -webkit-transform: scaleY(50%);
          transform: scaleY(50%);
}
.coupon-item .left {
  display: flex;
  flex-direction: column;
  justify-content: center;
  flex: 1;
  margin-left: 15px;
