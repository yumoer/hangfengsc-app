/* 优惠券面板 */
.mask {
  display: flex;
  align-items: flex-end;
  position: fixed;
  left: 0;
  top: 0px;
  bottom: 0;
  width: 100%;
  background: rgba(0, 0, 0, 0);
  z-index: 9995;
  transition: .3s;
}
.mask .mask-content {
  width: 100%;
  background: #f3f3f3;
  -webkit-transform: translateY(100%);
          transform: translateY(100%);
  transition: .3s;
  overflow-y: scroll;
}
.mask.none {
  display: none;
}
.mask.show {
  background: rgba(0, 0, 0, 0.4);
}
.mask.show .mask-content {
  -webkit-transform: translateY(0);
          transform: translateY(0);
}
/* 优惠券列表 */
.coupon-item {
  display: flex;
  flex-direction: column;
  margin: 20rpx 24rpx;
  background: #fff;
}
