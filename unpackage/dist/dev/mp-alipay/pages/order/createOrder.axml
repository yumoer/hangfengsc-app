<view class="container"><navigator class="address-section" url="/pages/address/address?source=1"><view class="order-content"><text class="yticon icon-shouhuodizhi"></text><view class="cen"><view class="top"><text class="name">{{addressData.title}}</text><text class="mobile">{{addressData.mobile}}</text></view><block a:if="{{addressData.addressName!==undefined}}"><text class="address">{{addressData.addressName+" "+addressData.place}}</text></block></view><text class="yticon icon-you"></text></view></navigator><view class="goods-content"><view class="goods-section"><view class="g-item" a:for="{{datas}}" a:for-item="data" a:for-index="index" a:key="index"><image src="{{data.default_image_url}}"></image><view class="right"><text class="title">{{data.title}}</text><text class="attr">无规格</text><view class="price-box"><text class="price" style="color:red;">{{"￥"+data.price}}</text><uni-number-box class="step" style="position:absolute;right:0;" vue-id="{{'746079d8-1-'+index}}" min="{{1}}" disabled="{{true}}" value="{{data.count}}" isMin="{{data.count===1}}" data-event-opts="{{[['^change',[['numberChange',['$event','$0',index],[[['datas','',index]]]]]]]}}" onChange="__e" onVueInit="__l"></uni-number-box></view></view></view></view><view class="yt-list"><navigator class="yt-list-cell" url="/pages/invoice/invoice?source=1"><text class="cell-tit clamp">发票</text><text class="cell-tip active">{{invoice}}</text><text class="yticon icon-you" style="font-size:14px;line-height:20px;"></text><text class="cell-more wanjia wanjia-gengduo-d"></text></navigator><block a:if="{{invoice!=='不开发票'}}"><view class="yt-list-cell desc-cell"><text class="cell-tit clamp">发票邮寄地址</text><input class="desc" type="text" placeholder="请填写发票具体邮寄地址" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','send_invoice_address','$event',[]],['info']]]]]}}" value="{{info.send_invoice_address}}" onInput="__e"/></view></block><view class="yt-list-cell"><text class="cell-tit clamp">运费</text><block a:if="{{allPrice!=price}}"><text class="cell-tip">{{"￥"+$root.g0}}</text></block><block a:else><text class="cell-tip">免邮</text></block></view><navigator data-event-opts="{{[['tap',[['goToPage',['/pages/coupon/exchange?source=1']]]]]}}" class="yt-list-cell" onTap="__e"><text class="cell-tit clamp">优惠券</text><text class="cell-tip active" style="margin-right:5px;">{{coupon}}</text><text class="yticon icon-you" style="font-size:14px;line-height:20px;"></text><text class="cell-more wanjia wanjia-gengduo-d"></text></navigator><view class="yt-list-cell desc-cell"><text class="cell-tit clamp">订单备注</text><input class="desc" type="text" placeholder="请填写备注信息" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['','desc','$event',[]]]]]]}}" value="{{desc}}" onInput="__e"/></view><view class="yt-list-cell"><text class="cell-tit clamp">支付方式</text><text class="cell-tip active" style="margin-right:5px;">在线支付</text></view><view style="height:40px;line-height:40px;"><text style="position:absolute;right:25px;"><text style="color:#999;">小计 :</text><text style="color:#333;margin:0 20rpx;">{{"共 "+datas.length+" 件"}}</text><text style="color:#EE1D23;font-weight:bold;">{{"￥"+$root.g1}}</text></text></view></view></view><view class="footer"><view class="price-content"><text>合计 :</text><text class="price-tip">￥</text><block a:if="{{$root.m0*count<100}}"><text class="price">{{$root.g2}}</text></block><block a:else><text class="price">{{$root.g3}}</text></block><text class="coupon">{{''+(price!==$root.m1?'含13.00元运费':'免邮')+''}}</text></view><text data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" class="submit" onTap="__e">提交订单</text></view></view>