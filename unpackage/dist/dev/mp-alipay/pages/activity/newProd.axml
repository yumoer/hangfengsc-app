<view class="content"><view class="header"><view class="goods"><block a:for="{{newProdList}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{index<3}}"><view data-event-opts="{{[['tap',[['navToPage',['$0'],[[['newProdList','',index]]]]]]]}}" class="item" onTap="__e"><image class="image" style="width:100px;height:100px;border-radius:20rpx;" src="{{item.image}}" mode></image><view class="price">{{''+item.text+''}}</view></view></block></block></view></view><view class="body"><block a:for="{{newProdList}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{index>2}}"><view data-event-opts="{{[['tap',[['navToPage',['$0'],[[['newProdList','',index]]]]]]]}}" class="item-list" onTap="__e"><view class="item-image"><image style="width:260rpx;height:260rpx;" src="{{item.image}}" mode></image></view><view class="item-right"><view class="logo"><image style="width:148rpx;height:48rpx;" src="http://47.94.106.106:8888/group1/M00/59/00/rBHxiGE5ePKAGmt1AAAlC8uSr5s7486228" mode></image></view><view class="title clamp2">{{''+item.title+''}}</view><view class="info"><view class="price">尝鲜价<text>{{item.text}}</text></view><view class="stock"><text>{{item.sales+"人付款"}}</text></view></view></view></view></block></block></view></view>